import 'package:flutter/material.dart';
import 'package:flutter/widgets.dart';
import 'package:lazyload/main.dart.copy';

import 'PdfRender.dart';

void main(List<String> args) async {
  var _simplePdfRender = await SimplePdfRender.open('1417.pdf');
  var a = await _simplePdfRender.getImage(1, 1).first;
  runApp(MaterialApp(
    title: 'lazyLoading',
    theme: ThemeData(primarySwatch: Colors.blue),
    home: a,
  ));
}
